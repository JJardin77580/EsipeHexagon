package Pionnieers.Hexagon;

import Pionnieers.Player;


public class DataHexagon {
	private Integer identifiant;
	private Player player;
	private int type;
	public enum Type {FOREST, PASTURE, FARMLAND, HILL, MOUNTAIN,DESERT, MARINA, SEA;};

	public DataHexagon(int identifiant,Player player, Type type) {
		this.identifiant = identifiant;
		this.setPlayer(player);
		this.type = type.ordinal();
	}

	public Integer getScore() {
		return identifiant;
	}

	public Player getPlayer() {
		return player;
	}

	public void setPlayer(Player player) {
		this.player = player;
	}
	
	
	
}
